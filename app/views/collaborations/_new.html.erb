<h4>Current Collaborator(s): </h4>
<div style="margin-left:30px;">
  <% wiki.collaborations.each do |c| %>
    <div class="row">
      <%= link_to 'X', @collaboration, class: "btn btn-danger btn-xs"%> <%= c.user.email %>
    </div>
  <% end %>
</div><br><br>

<%= form_for :collaborations do |f| %>
  <%= f.label :collaborations %>
  <%= f.select :collaborations, wiki.find_users, :include_blank => true, class: 'form-control' %><br><br>

  <%= link_to "Add Collaborator", "collaborations#create" %>
  <br><br><br>
<% end %>
